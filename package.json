{
  "name": "easyyclick",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@douyinfe/semi-ui": "2.68.3",
    "@remixicon/react": "4.4.0",
    "@semi-bot/semi-theme-easyy": "1.0.5",
    "bookmarks-parser": "^1.5.0",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "file-saver": "2.0.5",
    "idb-keyval": "6.2.1",
    "inobounce": "0.2.1",
    "jwt-decode": "4.0.0",
    "lodash.uniqby": "4.7.0",
    "openpgp": "5.11.2",
    "qrcode.react": "3.1.0",
    "ramda": "0.30.1",
    "react": "18.3.1",
    "react-baby-router": "0.2.2",
    "react-dom": "18.3.1",
    "react-error-boundary": "4.0.13",
    "react-fast-memo": "2.0.1",
    "usecat": "1.1.4",
    "webextension-polyfill": "0.12.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "4.3.3",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-sonarjs": "1.0.4",
    "prettier": "3.3.3",
    "rollup-plugin-visualizer": "5.12.0",
    "vite": "5.4.10",
    "vite-plugin-semi-theme": "0.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "cc": "node scripts/commit.js",
    "cs": "cp -a ../private-sharedjs/scripts/copy-shared.js scripts/copy-shared.js && node scripts/copy-shared.js semi",
    "start": "npm run cs && vite",
    "build": "npm run cs && vite build && npm run build-sw",
    "build-sw": "vite build --config vite.sw.config.js",
    "build-ext-bg": "vite build --config extension/vite.background.config.js",
    "build-ext": "npm run build-ext-bg && vite build --config extension/vite.config.js && cp -r dist-ext-bg/* dist-ext/ && cp extension/manifest.json dist-ext/manifest.json && cp -r public/icons dist-ext/icons && npm run zip-ext",
    "zip-ext": "cd dist-ext && zip -r ../easyyclick.zip . && cd ..",
    "dt": "node ./scripts/update-build-env.js dev && node ./scripts/upload.js",
    "dp": "node ./scripts/update-build-env.js prod && node ./scripts/upload.js",
    "vt": "node ./scripts/update-build-env.js dev && node ./scripts/update-version.js",
    "vp": "node ./scripts/update-build-env.js prod && node ./scripts/update-version.js",
    "lint": "eslint .",
    "ana": "vite --profile",
    "rr": "node scripts/react-versions.js"
  }
}
